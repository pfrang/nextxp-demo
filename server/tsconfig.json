{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "skipLibCheck": true,
    "moduleResolution": "Node",
    "lib": [
      "es6"
    ],
    "strict": true,
    "esModuleInterop": true,
    "outDir": "../dist/server",
    "allowJs": true,
    "noImplicitAny": true,
    "resolveJsonModule": true
  },
  "include": [
    "../next-env.d.ts",
    "./**.ts"
  ]
}
